<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.gr.qrapi.core.model.Contact" table="qr_contact">
		<id name="id" type="int">
			<column name="contact_id" />
			<generator class="native" />
		</id>
		<property name="accountId" type="int">
			<column name="account_id"></column>
		</property>
		<property name="firstName" type="java.lang.String">
			<column name="first_name" />
		</property>
		<property name="lastName" type="java.lang.String">
			<column name="last_name" />
		</property>
		<property name="emailAddress" type="java.lang.String">
			<column name="email_address" />
		</property>
		<property name="gender" type="java.lang.String">
			<column name="gender" />
		</property>
		<property name="phoneNumber" type="java.lang.String">
			<column name="phone_number" />
		</property>
		<property name="status" type="java.lang.String">
			<column name="status" />
		</property>
		<bag name="contactAddresses" inverse="false" table="qr_contact" lazy="false">
            <key>
                <column name="contact_id" />
            </key>
            <one-to-many class="com.gr.qrapi.core.model.ContactAddress" />
        </bag>
	</class>
</hibernate-mapping>

