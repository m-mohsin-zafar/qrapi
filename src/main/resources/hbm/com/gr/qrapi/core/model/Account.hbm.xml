<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gr.qrapi.core.model.Account" table="qr_account">
        <id name="id" type="int">
            <column name="account_id" />
            <generator class="native" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" />
        </property>
        <property name="emailAddress" type="java.lang.String">
            <column name="email_address" />
        </property>
        <property name="timeZone" type="java.lang.String">
            <column name="time_zone" />
        </property>
        <property name="userName" type="java.lang.String">
            <column name="user_name" />
        </property>
        <property name="passWord" type="java.lang.String">
            <column name="password" />
        </property>
        <bag name="contacts" inverse="false" table="qr_contact" lazy="false" >
            <key>
                <column name="account_id" />
            </key>
            <one-to-many class="com.gr.qrapi.core.model.Contact" />
        </bag>
        <bag name="alertProfiles" inverse="false" table="qr_alert_profile" lazy="false" >
            <key>
                <column name="account_id" />
            </key>
            <one-to-many class="com.gr.qrapi.core.model.AlertProfile" />
        </bag>
    </class>
</hibernate-mapping>